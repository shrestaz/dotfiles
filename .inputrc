# mappings for Ctrl-left-arrow and Ctrl-right-arrow for word moving
"\e[1;5C": forward-word
"\e[1;5D": backward-word
"\e[5C": forward-word
"\e[5D": backward-word
"\e\e[C": forward-word
"\e\e[D": backward-word
"\C-H":"\C-W"
TAB: menu-complete
set show-all-if-ambiguous on
set menu-complete-display-prefix on
"\C-f": menu-complete
set colored-stats on
set completion-ignore-case on
